<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="musicRockBean" class="ru.home.chernyadieva.music.RockMusic">
    </bean>

    <bean id="musicPlayer" class="ru.home.chernyadieva.music.player.MusicPlayer">
        <property name="music" ref="musicRockBean"/>

        <property name="author" value="Green Day"/>
        <property name="rating" value="8"/>
    </bean>
</beans>